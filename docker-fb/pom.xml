    <version>1.0</version>
  </parent>
  <artifactId>docker-fb</artifactId>
   <packaging>ear</packaging>
   <dependencies>
	     	<dependency>
			<groupId>beacon</groupId>
			<artifactId>fb-campaignfinisher</artifactId>
			<version>1.0</version>
			<type>war</type>
			</dependency>
			
				     	<dependency>
			<groupId>beacon</groupId>
			<artifactId>fb-cronjobs</artifactId>
			<version>1.0</version>
			<type>war</type>
			</dependency>
			
				     	<dependency>
			<groupId>beacon</groupId>
			<artifactId>fb-dltfileprocessor</artifactId>
			<version>1.0</version>
			<type>war</type>
			</dependency>
			
			
				     	<dependency>
			<groupId>beacon</groupId>

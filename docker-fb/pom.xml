			</dependency>
			
			
				     	<dependency>
			<groupId>beacon</groupId>
			<artifactId>fb-excludeprocessor</artifactId>
			<version>1.0</version>
			<type>war</type>
			</dependency>
			
			
				     	<dependency>
			<groupId>beacon</groupId>
			<artifactId>fb-fileupload</artifactId>
			<version>1.0</version>
			<type>war</type>
			</dependency>
 				     	<dependency>
			<groupId>beacon</groupId>
			<artifactId>fb-groupsprocessor</artifactId>
			<version>1.0</version>
			<type>war</type>
			</dependency>
 				     	<dependency>
			<groupId>beacon</groupId>
			<artifactId>fb-handoverstage</artifactId>
			<version>1.0</version>
			</dependency>
 				     	<dependency>
			<groupId>beacon</groupId>
			<artifactId>fb-initialstage</artifactId>
			<version>1.0</version>
			<type>war</type>
			</dependency>
			
			
			 				     	<dependency>
			<groupId>beacon</groupId>
			<artifactId>fb-inmemoryrefresh</artifactId>
			<version>1.0</version>
